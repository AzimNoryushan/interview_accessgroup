/*CRUD*/
//Create
POST /user/
    "FirstName": 
    "LastName":
    "Role"

//Update
PUT /user/{id}
    "FirstName": 
    "LastName":
    "Role"

//Delete
DELETE /user/{id}

// Retrieve all
GET /user/

// Retrieve by id
GET /user/{id}

